{
    "extends": ["react-app", "plugin:react/recommended"],
    "rules": {
        "semi": ["error", "always"], 
        "quotes": ["error", "single"], 
        "indent": ["error", 2],
        "no-unused-vars": "error", 
        "no-console": "warn",
        "react/jsx-uses-react": "error", 
        "react/jsx-uses-vars": "error",
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off" 
    }
  }